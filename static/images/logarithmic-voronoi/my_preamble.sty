\newcommand{\shiftleft}[2]{\makebox[0pt][r]{\makebox[#1][l]{#2}}}
\newcommand{\shiftright}[2]{\makebox[#1][r]{\makebox[0pt][l]{#2}}}
\newcommand{\myind}[1]{   \indep{\raise 7pt\hbox{\shiftleft{4pt}{{\tiny #1}}}}   }
\newcommand{\nmyind}[1]{   \indep{\raise 7pt\hbox{\shiftleft{4pt}{{\tiny #1}}}}{\raise .5pt\hbox{\shiftleft{10pt}{$\not$}}}   }
\newcommand{\myhatind}[1]{  \overset{\sim}{\indep}{\raise 7pt\hbox{\shiftleft{5pt}{{\tiny #1}}}}  }

\newcommand{\dind}{\myind{$\mathrm{d}$}}
